{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": "off",
    "semi": "off",
    "comma-dangle": "off",
    "indent": ["error", 2],
    "arrow-body-style": "off"
  }
}
